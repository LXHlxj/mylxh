<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.i.springboot.mapper.HrMapper">
    <select id="login" resultType="com.i.springboot.mode.UserHr" parameterType="java.lang.String">
        select username,password from hr where username =#{username} and password= #{password};
    </select>
</mapper>
